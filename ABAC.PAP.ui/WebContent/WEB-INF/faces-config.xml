<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<action-listener>org.primefaces.application.DialogActionListener</action-listener>
		<navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
		<view-handler>org.primefaces.application.DialogViewHandler</view-handler>
	</application>

	<validator>
		<validator-id>dataTypeValidator</validator-id>
		<validator-class>com.aislab.accesscontrol.core.ui.validator.DataTypeValidator</validator-class>
	</validator>

	<validator>
		<validator-id>stringValidator</validator-id>
		<validator-class>com.aislab.accesscontrol.core.ui.validator.StringValidator</validator-class>
	</validator>

</faces-config>
